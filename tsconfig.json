{
  "compilerOptions": {
    "target": "ESNext",
    "module": "nodenext",
    "strict": true,
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "paths": {
      "src/*": [
        "src/*"
      ]
    },
    "outDir": "dist",
    "moduleResolution": "nodenext",
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  }
}