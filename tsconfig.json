{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "strict": true,
    "skipLibCheck": true,
    "noImplicitAny": true,
    "sourceMap": true,
    "baseUrl": ".",
    "noEmit": true,
    "paths": {
      "src/*": [
        "src/*"
      ]
    },
    "outDir": "dist",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "jsx": "react-jsx"
  },
  "include": [
    "src",
    "forge.env.d.ts",
    "env.d.ts"
  ]
}